pipeline {
  agent any
  stages {
    stage('Test') {
      steps {
        echo 'start test stage'
        sleep 30
        echo 'fin test stage'
      }
    }

    stage('Build') {
      steps {
        echo 'start build stage'
        sleep 30
        echo 'fin build stage'
      }
    }

    stage('Deploy') {
      steps {
        echo 'start deploy stage'
        sleep 30
        echo 'fin delpoy stage'
      }
    }

  }
}